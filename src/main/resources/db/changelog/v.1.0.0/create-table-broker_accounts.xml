<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="create-table-broker_accounts" author="ForwardMoth">
        <createTable tableName="broker_accounts">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" primaryKeyName="broker_accounts_pkey"/>
            </column>
            <column name="is_IIA" type="boolean">
                <constraints nullable="false"/>
            </column>
            <column name="person_id" type="bigint">
                <constraints nullable="false" foreignKeyName="FK_broker_account_person"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>